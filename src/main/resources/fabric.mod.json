{
  "schemaVersion": 1,
  "name": "Meteor Litematica Printer",
  "id": "meteor-litematica-printer",
  "version": "${version}",
  "description": "Fast printer for litematica made with meteor.",
  "authors": [
    "kkllffaa"
  ],
  "contact": {
    "repo": "https://github.com/kkllffaa/meteor-litematica-printer"
  },
  "icon": "assets/meteor_litematica_printer/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      {
        "adapter": "kotlin",
        "value": "com.kkllffaa.meteor_litematica_printer.Addon"
      }
    ]
  },
  "mixins": [
    "mixins/meteor_litematica_printer.json"
  ],
  "depends": {
    "java": ">=21",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*",
    "fabric-language-kotlin": ">=1.13.7+kotlin.2.2.21"
  }
}
